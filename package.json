{
  "name": "react-router4-with-layouts",
  "version": "1.4.1",
  "main": "lib/index.js",
  "module": "lib/es/index.js",
  "jsnext:main": "lib/src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nfq-eta/react-router4-with-layouts.git"
  },
  "author": "Vytenis Kuƒçiauskas",
  "license": "MIT",
  "files": [
    "lib/",
    "typings/",
    "src/"
  ],
  "scripts": {
    "cm": "git-cz",
    "lint": "tslint -c tslint.json -p tsconfig.json -t stylish",
    "build:example": "cd example && yarn --no-lockfile && yarn build",
    "prebuild": "rimraf lib typings example/build && yarn lint",
    "build": "tsc -d && tsc -m es6 -d --outDir lib/es && tsc -m esnext -t esnext -d --outDir lib/src && tsc -m umd -t es5 --outDir lib/umd",
    "postbuild": "dts-generator --name react-router4-with-layouts --main react-router4-with-layouts/index --project . --indent '    ' --out typings/index.d.ts",
    "start": "tsc -p ./tsconfig.json --watch",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "predeploy": "yarn build:example",
    "deploy": "gh-pages -d example/build"
  },
  "devDependencies": {
    "@types/prop-types": "15.7.1",
    "@types/react": "16.8.23",
    "@types/react-router": "5.0.3",
    "@types/react-router-dom": "4.3.4",
    "commitizen": "^3.1.1",
    "conventional-changelog-cli": "2.0.21",
    "cz-conventional-changelog": "2.1.0",
    "dts-generator": "3.0.0",
    "gh-pages": "^2.0.1",
    "rimraf": "2.6.3",
    "tslint": "5.18.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-react": "4.0.0",
    "typescript": "3.5.3"
  },
  "peerDependencies": {
    "prop-types": "*",
    "react": "*",
    "react-router-dom": "*"
  },
  "typings": "./typings/index.d.ts",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "tslib": "1.9.0"
  }
}
