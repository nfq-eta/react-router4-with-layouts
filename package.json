{
  "name": "react-router4-with-layouts",
  "version": "1.2.3",
  "main": "lib/index.js",
  "module": "lib/es/index.js",
  "jsnext:main": "lib/src/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:nfq-eta/react-router4-with-layouts.git"
  },
  "homepage": "https://github.com/nfq-eta/react-router4-with-layouts",
  "author": "Vytenis Kuƒçiauskas",
  "license": "MIT",
  "files": [
    "lib/",
    "typings/"
  ],
  "scripts": {
    "cm": "git-cz",
    "lint": "tslint -c tslint.json -p tsconfig.json -t stylish",
    "build:example": "cd example && yarn && yarn build --no-lockfile",
    "prebuild": "rimraf lib typings example/build && yarn lint",
    "build": "tsc -d && tsc -m es6 -d --outDir lib/es && tsc -m esnext -t esnext -d --outDir lib/src && tsc -m umd -t es5 --outDir lib/umd",
    "postbuild": "dts-generator --name react-router4-with-layouts --main react-router4-with-layouts/index --project . --indent '    ' --out typings/index.d.ts",
    "start": "tsc -p ./tsconfig.json --watch",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "predeploy": "cd example && yarn && yarn build",
    "deploy": "gh-pages -d example/build",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "@types/prop-types": "15.5.2",
    "@types/react": "16.0.31",
    "@types/react-router-dom": "4.2.3",
    "commitizen": "2.9.6",
    "conventional-changelog-cli": "1.3.5",
    "cz-conventional-changelog": "2.1.0",
    "dts-generator": "2.1.0",
    "rimraf": "2.6.2",
    "tslint": "5.8.0",
    "tslint-config-airbnb": "5.4.2",
    "tslint-react": "3.2.0",
    "typescript": "2.6.2"
  },
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-router-dom": "^4.0.0"
  },
  "typings": "./typings/index.d.ts",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
